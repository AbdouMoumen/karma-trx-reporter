{
    "editor.renderWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules/": true,
        "**/*.js": { "when": "$(basename).ts" },
        "**/*.js.map": {"when": "$(basename)"}
    }
}